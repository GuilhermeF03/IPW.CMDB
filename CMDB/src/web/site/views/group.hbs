<h1>{{name}}</h1>
<p>{{description}}</p>
<p>Total movies: {{number of movies}}</p>
<p>Total duration: {{total duration}}</p>
{{!-- upsdate field todo --}}


  {{#each movies}}
   <li>
    <a href="/movies/{{id}}">
        <img src="{{image}}" alt="{{title}}">
    </a>
    <p>{{duration}}</p>
    {{!-- verificar --}}
    <from action="/groups/:groupId/{{id}}/delete" method="POST">
        <input type="submit" value="Delete movie"/>
   </li>
  {{/each}}
